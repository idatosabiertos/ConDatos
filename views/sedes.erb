<link href="css/leaflet.css" rel="stylesheet">
<section class="main_container" id="sedes_page">
  <a id="ogp_logo_mobile" href="#">ConDatos/AbreLatam</a>
  <div id="sedes_content">
    <%= t.hqs.content1 %>
  </div>

  <div id="map">
  </div>

  <div class="map_info_sidebar" id="radisson">
    <div class="inner">
      <a href="#" class="close" id="close_radisson"><%= t.layout.close %></a>
      <div class="title">
        <h3><%= t.radisson.title %></h3>
      </div>

      <div class="direction">
        <h4><%= t.hqs.address %></h4>
        <p><%= t.radisson.address %></p>
      </div>

      <div class="general_info">
        <p><b><%= t.radisson.description1 %></b></p>
        <p><%= t.radisson.description2 %></p>
        <h4 class="title_activity"><%= t.hqs.activities %></h4>
        <div class="activity">
          <p><%= t.radisson.date1 %></p>
          <small><%= t.radisson.event1 %></small>
        </div>
        <div class="activity">
          <p><%= t.radisson.date2 %></p>
          <small><%= t.radisson.event2 %></small>
        </div>
      </div>
    </div>

    <div class="apps_info">
      <p><%= t.radisson.apps_info %></p>
    </div>
  </div>

</section>
<script src="js/leaflet.js" type="text/javascript"></script>
<script src="js/sedes.js" type="text/javascript"></script>
